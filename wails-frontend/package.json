    "eslint": "^8.39.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
